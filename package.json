      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.8.0",
    "electron-is-dev": "^1.1.0",
    "nodemon": "^2.0.1",
    "react-app-rewired": "^2.1.4",
    "babel-plugin-import": "^1.12.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "wait-on": "^3.3.0"
