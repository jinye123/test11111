{
  "name": "jinye-react",
  "version": "0.1.0",
  "main": "main.js",
  "private": true,
  "dependencies": {
    "antd": "^3.24.2",
    "axios": "^0.21.1",
    "electron": "^7.1.3",
    "normalize.css": "^8.0.1",
    "react": "^16.10.2",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "store": "^2.0.12"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
